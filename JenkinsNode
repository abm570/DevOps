pipeline{
  agent any

  stages{
        stage('Checkout') {
              steps {
                    git branch: 'main', url: 'https://github.com/abm570/DevOps/'
              }
        }

        stage('Build') {
              steps {
                    echo 'Building....'
              }
        }

        stage('Execute') {
              steps {
                  sh 'node Hello.js'
              }
         }
    }
}
