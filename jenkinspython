pipeline {
    agent any

    tools {
        python 'python3'
    }

    stages {
        stage('Checkout') {
            steps {
                git branch: 'main', url: 'https://github.com/abm570/DevOps/'
            }
        }

        stage('Compile') {
            steps {
                echo 'Compiling....'
                sh 'python3 hello.py'
            }
        }
      }
    }
}
